{% extends 'question/admin/base.html.twig' %}

{% block body %}
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-md-6">
                <h1 class="text-center">{{ title }}</h1>
                {{ form_start(form, {'attr': {'class': 'form-contact'}}) }}
                {{ form_row(form.name) }}
                {{ form_row(form.subject) }}
                {{ form_row(form.emailAddress) }}
                {{ form_row(form.message) }}
                <div class="text-center">
                    {{ form_row(form.send) }}
                </div>
                {{ form_end(form) }}
            </div>
        </div>
    </div>
{% endblock %}
